grammar Ngs

rule expression
  space? '(' body ')' space? <Expression>
end

rule body
  (expression / me / (friends / people) / likeand / likes / thing / space )* <Body>
end

rule me
  "me" <Me>
end

rule friends
  "friends" <Friends>
end

rule likes
  "who like" <Likes>
end

rule likeand
  likes space thing space "and" space thing <LikeAnd>
end

rule thing
  [a-zA-Z]+ <Thing>
end

rule people
  "people" <People>
end

rule space
  [\s]+
end

end