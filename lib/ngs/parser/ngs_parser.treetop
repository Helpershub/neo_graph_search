grammar Ngs

rule expression
  space? '(' body ')' space? <Expression>
end

rule body
  (expression / me / friends / likes / thing / space )* <Body>
end

rule me
  "me" <Me>
end

rule friends
  "friends" <Friends>
end

rule likes
  "who like" <Likes>
end

rule thing
  [a-zA-Z]+ <Thing>
end

rule space
  [\s]+
end

end